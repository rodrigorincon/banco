<%= form_tag(withdraw_accounts_path, method: :post, class: "form-horizontal") do %>
	<div class="field">
		<label> Defina o valor a sacar </label><br />
		<%= number_field_tag :withdraw_value, number_with_precision(0.00, precision: 2), step: 0.01 %>
	</div>

	<%= submit_tag "Sacar", class: "btn btn-primary" %>
<% end %>